<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.edu_gate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TutorProfileActivity"
            android:label="Tutor Profile"></activity>
        <activity
            android:name=".SignupEducationActivity"
            android:label="Educational Information" />
        <activity
            android:name=".SignupPersonalActivity"
            android:label="Personal Information" />
        <activity
            android:name=".SignupEmailPasswordActivity"
            android:label="Sign Up" />
        <activity
            android:name=".SearchActivity"
            android:label="Search"
            android:parentActivityName=".StartupActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="Login"
            android:parentActivityName=".StartupActivity" />
        <activity android:name=".StartupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>