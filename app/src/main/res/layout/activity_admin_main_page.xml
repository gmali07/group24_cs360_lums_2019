<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminMainPageActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:background="@color/navy">
    </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/re"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="40dp">
            <LinearLayout
                android:id="@+id/lik"
                android:layout_marginTop="80dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_height="130dp"
                android:gravity="center">
                <LinearLayout
                    android:layout_width="0dp"
                    android:background="@drawable/et2"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:orientation="vertical">
                    <!--<Button-->
                        <!--android:id="@+id/search"-->
                        <!--android:layout_width="match_parent"-->
                        <!--android:layout_height="match_parent"-->
                        <!--android:background="@drawable/et"-->
                        <!--android:text="SEARCH"></Button>-->
                    <ImageButton
                        android:id="@+id/search"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:padding="0dp"
                        android:src="@drawable/search2" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/et2"

                    android:orientation="vertical">
                    <Button
                        android:id="@+id/verify_profile"
                        android:background="@drawable/et"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="VERIFY PROFILES" />

                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"
            android:id="@+id/rel"
            android:layout_below="@+id/re">

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_height="130dp"
                android:gravity="center">
                <LinearLayout
                    android:layout_width="0dp"
                    android:background="@drawable/et2"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/rate_tutor"
                        android:layout_width="match_parent"
                        android:background="@drawable/et"
                        android:layout_height="match_parent"
                        android:text="RATE TUTOR"></Button>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/et2"

                    android:orientation="vertical">
                    <Button
                        android:id="@+id/change_password"
                        android:layout_width="match_parent"
                        android:background="@drawable/et"
                        android:layout_height="match_parent"
                        android:text="CHANGE PASSWORD" />

                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"
            android:id="@+id/olp"
            android:layout_below="@+id/rel">


            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_height="130dp"
                android:gravity="center">
                <LinearLayout
                    android:layout_width="0dp"
                    android:background="@drawable/et2"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:orientation="vertical">
                    <Button
                        android:padding="5dp"
                        android:id="@+id/add_new_administrator"
                        android:layout_width="match_parent"
                        android:background="@drawable/et"
                        android:layout_height="match_parent"
                        android:text="ADD NEW ADMINISTRATOR"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/et2"

                    android:orientation="vertical">
                    <Button
                        android:id="@+id/delete_tutor_profile"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="DELETE TUTOR PROFILE"
                        android:background="@drawable/et" />

                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>
        <Button
            android:id="@+id/signoutButton"
            android:textColor="@color/white"
            android:layout_marginTop="30dp"
            android:layout_below="@+id/olp"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn"
            android:text="Signout" />





